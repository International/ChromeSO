{
  "name": "StackOverflow notifier",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "description": "This will help you keep track with your StackOverflow notifications.",
  "browser_action": {
    "default_icon": "icon_16.png"
  },
  "icons" : {
    "16"  : "icon_16.png",
  },
  "web_accessible_resources" : [ "icon_16.png" ],
  "permissions": [ "notifications", "http://*/*", "https://*/*" ],
  "background": {
    "scripts" : ["jquery-1.7.2.min.js","background.js"]
  }
}
